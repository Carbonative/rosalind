{
  "name": "rosalind",
  "productName": "Rosalind",
  "version": "0.0.199",
  "private": true,
  "description": "Rosalind",
  "author": {
    "name": "Albert Ronald Roland Zak",
    "email": "security@rslnd.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/albertzak/rosalind"
  },
  "homepage": "https://update.rslnd.com",
  "main": "main/main.js",
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "electron-packager": "^12.1.1",
    "shelljs": "^0.8.2"
  },
  "dependencies": {
    "@sentry/electron": "^0.8.1",
    "chokidar": "^2.0.4",
    "electron-localshortcut": "^3.1.0",
    "electron-updater": "^4.0.6",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "iconv-lite": "^0.4.24",
    "lodash": "^4.17.11",
    "opn": "^5.3.0",
    "pkginfo": "^0.4.1",
    "request": "^2.88.0",
    "temp": "^0.8.3",
    "windows-shortcuts-appid": "^0.3.2",
    "winston": "^2.4.4",
    "winston-papertrail": "^1.0.5"
  },
  "build": {
    "appId": "com.rslnd.rosalind",
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "files": [
      "**/*",
      "assets/appicon.ico"
    ],
    "extraResources": [
      "assets/eoswinPrinter.reg",
      "assets/generateEoswinReports.exe",
      "assets/focus.exe"
    ],
    "generateUpdatesFilesForAllChannels": true,
    "readonly": true,
    "forceCodeSigning": true,
    "win": {
      "publisherName": [
        "Albert Ronald Roland Zak"
      ],
      "icon": "assets/appicon.ico"
    },
    "nsis": {
      "createDesktopShortcut": false,
      "createStartMenuShortcut": false,
      "language": 7
    },
    "publish": {
      "provider": "github",
      "private": false
    }
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps",
    "start": "./node_modules/.bin/electron .",
    "pack": "./compileAutoIt.js && ./node_modules/.bin/electron-builder --dir",
    "build": "./node_modules/.bin/electron-builder --x64",
    "release": "build",
    "compile": "node ./compileAutoIt.js",
    "tag": "node tag.js | /bin/sh"
  }
}
