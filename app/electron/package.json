{
  "name": "rosalind",
  "productName": "Rosalind",
  "version": "0.0.243",
  "private": true,
  "description": "Rosalind",
  "author": {
    "name": "Fixpoint Systems GmbH",
    "email": "security@fxp.at"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rslnd/rosalind"
  },
  "homepage": "https://update.rslnd.com",
  "main": "main/main.js",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-packager": "^15.1.0",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "@sentry/electron": "^2.0.1",
    "chokidar": "^3.4.2",
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^4.3.5",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "iconv-lite": "^0.6.2",
    "lodash": "^4.17.20",
    "logdna": "^3.5.1",
    "pdf-to-printer": "^1.4.1",
    "pkginfo": "^0.4.1",
    "request": "^2.88.2",
    "temp": "^0.9.1"
  },
  "build": {
    "appId": "com.rslnd.rosalind",
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "files": [
      "**/*",
      "assets/appicon.ico"
    ],
    "extraResources": [
      "assets/eoswinPrinter.reg",
      "assets/generateEoswinReports.exe",
      "assets/focus.exe"
    ],
    "generateUpdatesFilesForAllChannels": true,
    "forceCodeSigning": true,
    "win": {
      "publisherName": [
        "Fixpoint Systems GmbH"
      ],
      "icon": "assets/appicon.ico"
    },
    "nsis": {
      "createDesktopShortcut": false,
      "createStartMenuShortcut": false,
      "language": 7
    },
    "publish": {
      "provider": "github",
      "private": false
    }
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps",
    "start": "./node_modules/.bin/electron .",
    "pack": "./compileAutoIt.js && ./node_modules/.bin/electron-builder --dir",
    "build": "./node_modules/.bin/electron-builder --x64",
    "release": "./node_modules/.bin/electron-builder build --x64",
    "compile": "node ./compileAutoIt.js",
    "tag": "node tag.js | /bin/sh"
  }
}
