.panel.panel-default
  .panel-body
    h4 #{firstName} #{lastName}
      if privatePatient
        small {{_ 'inboundCalls.private'}}
      else
        small {{_ 'inboundCalls.insurance'}}
    h5 #{telephone}

    blockquote
      p #{note}

    footer
      span.text-muted {{time createdAt}} &middot; {{livestamp createdAt}}

  .panel-footer
    a.resolve(href='#') {{_ 'inboundCalls.resolve'}}
