{
  "name": "rosalind-development",
  "private": true,
  "scripts": {
    "start": "docker-compose -f docker-compose.yml -f docker-compose.development.yml up",
    "start:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "shell": "docker exec -it rosalind_meteor_1 meteor shell",
    "docker-compose": "docker-compose -f docker-compose.yml -f docker-compose.development.yml",
    "server": "docker-compose -f docker-compose.yml -f docker-compose.development.yml run meteor",
    "server:npm": "docker-compose -f docker-compose.yml -f docker-compose.development.yml run meteor meteor npm",
    "server:npm:shrink": "npm run server:npm:shrinkwrap && npm run server:npm:shrinkpack",
    "server:npm:shrinkwrap": "docker-compose -f docker-compose.yml -f docker-compose.development.yml run meteor meteor npm shrinkwrap --dev",
    "server:npm:shrinkpack": "docker-compose -f docker-compose.yml -f docker-compose.development.yml run meteor ./node_modules/.bin/shrinkpack",
    "test": "node_modules/mocha/bin/mocha 'app/meteor/imports/**/*.test.coffee' --recursive --require app/meteor/tests/mocha/helper.js --compilers coffee:coffee-script/register",
    "test:acceptance": "./node_modules/coffee-script/bin/coffee ./test.coffee",
    "lint": "./node_modules/coffeelint/bin/coffeelint --quiet .",
    "client:start": "cd app/electron/ && node_modules/.bin/grunt",
    "client:build": "cd app/electron/ && node_modules/.bin/grunt grunt build"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "chai-datetime": "^1.4.1",
    "chimp": "^0.34.1",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.14.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "engines": {
    "node": "^0.12.7"
  }
}
